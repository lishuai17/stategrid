	<delete id="deleteResultByDate" parameterType="String">
		delete from CBPM_RESULET where mdate=#{mdate}
	</delete>
	 
	 
	<select id="selectSheetOfResultByArea" resultType="String">
		select distinct dsheet from CBPM_RESULET where area=#{area} and mdate=#{mdate}
	</select>
	 
	 
	<select id="selectResultByParam" resultType="ResultPo">
		select <include refid="daclareInfo" />
			from CBPM_RESULET 
			where area=#{area} and mdate=#{mdate} and dtype=#{dtype} 
		 <if test="mname!=null and mname!='' ">
			 and mname=#{mname}
		 </if>
